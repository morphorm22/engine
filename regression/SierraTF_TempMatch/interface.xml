<?xml version="1.0"?>
<include filename="defines.xml" />
<Console>
  <Enabled>true</Enabled>
  <Verbose>true</Verbose>
</Console>
<Performer>
  <Name>platomain_1</Name>
  <Code>platomain</Code>
  <PerformerID>0</PerformerID>
</Performer>
<Performer>
  <Name>sierra_tf_2</Name>
  <Code>sierra_tf</Code>
  <PerformerID>1</PerformerID>
</Performer>
<Performer>
  <PerformerID>2</PerformerID>
  <For var="I" in="Parameters">
    <Name>plato_esp_{I}</Name>
  </For>
</Performer>
<SharedData>
  <Name>Design Parameters</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>platomain_1</OwnerName>
  <UserName>platomain_1</UserName>
  <UserName>sierra_tf_2</UserName>
</SharedData>
<SharedData>
  <Name>Lower Bound Value</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>platomain_1</OwnerName>
  <UserName>platomain_1</UserName>
</SharedData>
<SharedData>
  <Name>Lower Bound Vector</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>platomain_1</OwnerName>
  <UserName>platomain_1</UserName>
</SharedData>
<SharedData>
  <Name>Upper Bound Value</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>platomain_1</OwnerName>
  <UserName>platomain_1</UserName>
</SharedData>
<SharedData>
  <Name>Upper Bound Vector</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>platomain_1</OwnerName>
  <UserName>platomain_1</UserName>
</SharedData>
<For var="I" in="Parameters">
  <SharedData>
    <Name>Parameter Sensitivity {I}</Name>
    <Type>Scalar</Type>
    <Layout>Global</Layout>
    <Dynamic>true</Dynamic>
    <OwnerName>plato_esp_{I}</OwnerName>
    <UserName>platomain_1</UserName>
    <UserName>sierra_tf_2</UserName>
  </SharedData>
</For>
<SharedData>
  <Name>Criterion Value - criterion_1_service_2_scenario_1</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>sierra_tf_2</OwnerName>
  <UserName>platomain_1</UserName>
</SharedData>
<SharedData>
  <Name>Criterion Gradient - criterion_1_service_2_scenario_1</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>sierra_tf_2</OwnerName>
  <UserName>platomain_1</UserName>
</SharedData>
<SharedData>
  <Name>Objective Value</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>platomain_1</OwnerName>
  <UserName>platomain_1</UserName>
</SharedData>
<SharedData>
  <Name>Objective Gradient</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>platomain_1</OwnerName>
  <UserName>platomain_1</UserName>
</SharedData>
