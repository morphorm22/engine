<ParameterList name="Problem">
  <Parameter name="Physics"            type="string"  value="Plato Driver"/>
  <Parameter name="Debug"              type="bool"    value="false"/>
  <Parameter name="Spatial Dimension"  type="int"     value="2"/>
  <Parameter name="Input Mesh"         type="string"  value="./bolted_bracket.exo"/>

  <ParameterList name="Plato Problem">
    <Parameter name="Physics"            type="string"  value="Plasticity"/>
    <Parameter name="PDE Constraint"     type="string"  value="Elliptic"/>
    <Parameter name="Debug"              type="bool"    value="false"/>

    <ParameterList name='Criteria'>  
      <ParameterList name='Elastic Work'>
        <Parameter name='Type' type='string' value='Weighted Sum'/>
        <Parameter name='Functions' type='Array(string)' value='{My Total Work}'/>
        <Parameter name='Weights' type='Array(double)' value='{-0.01}'/>
      </ParameterList>
      <!-- <ParameterList name='My Elastic Work'>
        <Parameter name='Type'                 type='string' value='Scalar Function'/>
        <Parameter name='Scalar Function Type' type='string' value='Elastic Work'/>
        <Parameter name='Exponent'             type='double' value='3.0'/>
        <Parameter name='Minimum Value'        type='double' value='1.0e-8'/>
      </ParameterList> -->
      <!-- <ParameterList name='My Plastic Work'>
        <Parameter name='Type'                 type='string' value='Scalar Function'/>
        <Parameter name='Scalar Function Type' type='string' value='Plastic Work'/>
        <Parameter name='Exponent'             type='double' value='2.0'/>
        <Parameter name='Minimum Value'        type='double' value='1.0e-8'/>
      </ParameterList> -->
      <ParameterList name='My Total Work'>
        <Parameter name='Type'                 type='string' value='Scalar Function'/>
        <Parameter name='Scalar Function Type' type='string' value='Total Work'/>
        <Parameter name='Exponent'             type='double' value='2.0'/>
        <Parameter name='Minimum Value'        type='double' value='1.0e-6'/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Spatial Model">
      <ParameterList name="Domains">
        <ParameterList name="Design Volume">
          <Parameter name="Element Block" type="string" value="block_1"/>
          <Parameter name="Material Model" type="string" value="Unobtainium"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Material Models">
      <Parameter  name='Pressure Scaling' type='double' value='100.0'/>
      <ParameterList name="Unobtainium">
        <ParameterList name='Isotropic Linear Elastic'>
          <Parameter name='Mass Density' type='double' value='8e-9'/>
          <Parameter  name='Poissons Ratio' type='double' value='0.3'/>
          <Parameter  name='Youngs Modulus' type='double' value='74633.0'/>
        </ParameterList>
        <ParameterList name='Plasticity Model'>                                             
          <ParameterList name='J2 Plasticity'>                                                
            <Parameter  name='Hardening Modulus Isotropic' type='double' value='2000.0'/>      
            <Parameter  name='Hardening Modulus Kinematic' type='double' value='1.0'/>      
            <Parameter  name='Initial Yield Stress' type='double' value='344.0'/>             
            <Parameter  name='Elastic Properties Penalty Exponent' type='double' value='2'/>  
            <Parameter  name='Elastic Properties Minimum Ersatz' type='double' value='1e-6'/> 
            <Parameter  name='Plastic Properties Penalty Exponent' type='double' value='1.5'/>
            <Parameter  name='Plastic Properties Minimum Ersatz' type='double' value='1e-3'/> 
          </ParameterList>                                                                    
        </ParameterList>                                                                      
      </ParameterList>
    </ParameterList>

    <ParameterList name='Elliptic'>
      <!-- <Parameter name="Plottable" type="Array(string)" value="{accumulated plastic strain, principal stresses, deviatoric stress, cauchy stress}"/> -->
      <ParameterList name='Penalty Function'>
        <Parameter name='Type' type='string' value='SIMP'/>
        <Parameter name='Exponent' type='double' value='2.0'/>
        <Parameter name='Minimum Value' type='double' value='1.0e-6'/>
      </ParameterList>
    </ParameterList>

    <!-- <ParameterList name='State Gradient Projection'>
      <ParameterList name='Penalty Function'>
        <Parameter name='Type' type='string' value='SIMP'/>
        <Parameter name='Exponent' type='double' value='2.0'/>
        <Parameter name='Minimum Value' type='double' value='1.0e-6'/>
        <Parameter name='Additive Continuation' type='double' value='0.1'/>
      </ParameterList>
    </ParameterList> -->

    <ParameterList name='Time Stepping'>
      <Parameter name='Expansion Multiplier'           type='double' value='1.25'/>
      <Parameter name='Initial Num. Pseudo Time Steps' type='int'    value='4'/>
      <Parameter name='Maximum Num. Pseudo Time Steps' type='int'    value='8'/>
    </ParameterList>

    <ParameterList name='Newton-Raphson'>
      <Parameter name='Stop Measure' type='string' value='residual'/>
      <Parameter name='Stopping Tolerance' type='double' value='1e-8'/>
      <Parameter name='Maximum Number Iterations' type='int' value='25'/>
    </ParameterList>

    <!--ParameterList  name="Natural Boundary Conditions">
      <ParameterList  name="Traction Vector Boundary Condition">
        <Parameter  name="Type"     type="string"        value="Uniform"/>
        <Parameter  name="Values"   type="Array(string)" value="{0.0, -2e2*t, 0.0}"/>
        <Parameter  name="Sides"    type="string"        value="load"/>
      </ParameterList>
    </ParameterList-->

    <ParameterList  name="Essential Boundary Conditions">

      <ParameterList  name="X Symm Boundary Condition">
        <Parameter  name="Type"     type="string" value="Zero Value"/>
        <Parameter  name="Index"    type="int"    value="0"/>
        <Parameter  name="Sides"    type="string" value="symmetry_x"/>
      </ParameterList>

      <ParameterList  name="Y Roller Boundary Condition">
        <Parameter  name="Type"     type="string" value="Zero Value"/>
        <Parameter  name="Index"    type="int"    value="1"/>
        <Parameter  name="Sides"    type="string" value="fixed_y"/>
      </ParameterList>

      <ParameterList  name="Y Disp Boundary Condition">
        <Parameter  name="Type"     type="string" value="Time Dependent"/>
        <Parameter  name="Index"    type="int"    value="1"/>
        <Parameter  name="Sides"    type="string" value="load_y"/>
        <Parameter  name="Function" type="string" value="-0.5*t"/>
      </ParameterList>

    </ParameterList>

  </ParameterList>

</ParameterList>

